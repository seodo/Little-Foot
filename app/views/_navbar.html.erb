<nav>
    <% if current_user %>
    <%= link_to "Logout", logout_path, method: :delete %>
    <%= link_to current_user.full_name, user_path(current_user) %>
    <% else %>
    <%= link_to "Log In", login_path %>
    <%= link_to "Register", new_user_path %>
    <%= link_to "Home", root_path %>
    <%end%>
</nav>
